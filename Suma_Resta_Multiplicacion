#include<stdio.h>

int main (void){
	
	int a,b,sum,dif,mul;
	
	printf("Indica el valor del primer numero: \n");
	scanf("%i",&a);
	
	printf("Indica el valor del segundo numero: \n ");
	scanf("%i", &b);
	
	sum=a+b;
	printf("\nLa suma de los dos numeros es: %i",sum); 
	
	
	dif=a-b;
	printf("\nLa resta de los dos numeros es: %i",dif); 
	
	mul=a*b;
	printf("\nLa multiplicacion de los dos numeros es: %i",mul); 
	 
	return 0;
}
