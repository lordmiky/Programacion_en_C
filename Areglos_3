#include<stdio.h>
void displayNumbers(int num[2][2]);
int main(){
	int num[2][2];
	printf("Ingrese 4 numeros enteros\n");
	for(int i=0; i<2; ++i){
		for(int j=0; j<2; ++j){
			scanf("%d",&num[i][j]);
		}
	}
displayNumbers(num);
return 0;
}

void displayNumbers(int num[2][2]){
	printf("mostrando datos\n");
	for(int i=0; i<2; ++i){
		for(int j=0; j<2; ++j){
			printf("%d\n",num[i][j]);
		}
		
	}
}
